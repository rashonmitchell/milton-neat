{{ section.settings.custom-liquid }}

{% schema %}
{
	"name": "t:sections.custom-liquid.name",
	"tag": "section",
	"class": "spaced-section",
	"settings": [
		{
			"type": "liquid",
			"id": "custom-liquid",
			"label": "t:sections.custom-liquid.settings.custom-liquid.label",
			"info": "t:sections.custom-liquid.settings.custom-liquid.info"
		}
	],
	"presets": [
		{
			"name": "t:sections.custom-liquid.presets.name"
		}
	]
}
{% endschema %}
